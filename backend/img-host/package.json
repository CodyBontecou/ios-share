{
  "name": "img-host",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:init": "./scripts/init-db.sh",
    "db:migrate": "wrangler d1 execute imagehost --file=./migrations/0001_initial_schema.sql",
    "db:migrate:local": "wrangler d1 execute imagehost --local --file=./migrations/0001_initial_schema.sql",
    "db:migrate:auth": "wrangler d1 execute imagehost --file=./migrations/0002_auth_enhancements.sql",
    "db:migrate:auth:local": "wrangler d1 execute imagehost --local --file=./migrations/0002_auth_enhancements.sql",
    "db:migrate:export": "wrangler d1 execute imagehost --file=./migrations/0003_export_jobs.sql",
    "db:migrate:export:local": "wrangler d1 execute imagehost --local --file=./migrations/0003_export_jobs.sql",
    "db:migrate:rate-limiting": "wrangler d1 execute imagehost --file=./migrations/0004_rate_limiting.sql",
    "db:migrate:rate-limiting:local": "wrangler d1 execute imagehost --local --file=./migrations/0004_rate_limiting.sql",
    "db:migrate:analytics": "wrangler d1 execute imagehost --file=./migrations/0005_analytics_tables.sql",
    "db:migrate:analytics:local": "wrangler d1 execute imagehost --local --file=./migrations/0005_analytics_tables.sql",
    "db:query": "wrangler d1 execute imagehost --command",
    "db:query:local": "wrangler d1 execute imagehost --local --command",
    "db:tables": "wrangler d1 execute imagehost --command=\"SELECT name FROM sqlite_master WHERE type='table';\"",
    "db:tables:local": "wrangler d1 execute imagehost --local --command=\"SELECT name FROM sqlite_master WHERE type='table';\""
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.24.0"
  }
}
